    "target": "ES6",
    "module": "ESNext",
    "jsx": "react-jsx",
    "lib": ["DOM", "ESNext"],
    "types": ["vite/client"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
